{
  "name": "@xing.wu/form",
  "version": "3.0.2",
  "description": "表单二次封装",
  "main": "dist/xing-wu-form.esm.js",
  "browser": "dist/xing-wu-form.esm.js",
  "module": "dist/xing-wu-form.esm.js",
  "unpkg": "dist/xing-wu-form.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": [
    "*.css",
    "*.vue"
  ],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "page": "vue-cli-service build dev/serve.js",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --bundleConfigAsCjs",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs --bundleConfigAsCjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es --bundleConfigAsCjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife --bundleConfigAsCjs"
  },
  "devDependencies": {
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@vue/cli-plugin-babel": "^5.0.9",
    "@vue/cli-service": "^5.0.9",
    "@vue/compiler-sfc": "^3.5.21",
    "core-js": "^3.45.1",
    "cross-env": "^10.0.0",
    "element-plus": "^2.11.3",
    "minimist": "^1.2.8",
    "postcss": "^8.5.6",
    "rimraf": "^6.0.1",
    "rollup": "^4.52.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "vue": "^3.5.21"
  },
  "peerDependencies": {
    "element-plus": "^2.2.0",
    "vue": "^3.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}
